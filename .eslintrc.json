{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommened",
        "plugin:jasx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "rules": {
        "react/prop-types": 0
    },
    "plugins": [
        "react",
        "import",
        "jasx-a11y"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}